{
    "redirects": [
      {
        "source": "/index.html",
        "destination": "/",
        "permanent": true
      },
      {
        "source": "/(.*).html",
        "destination": "/$1",
        "permanent": true
      },
      {
        "source": "http://www.koonmow.com/:path*",
        "destination": "https://koonmow.com/:path*",
        "permanent": true
      },
  {
        "source": "https://www.koonmow.com/:path*",
        "destination": "https://koonmow.com/:path*",
        "permanent": true
      },
      {
        "source": "http://koonmow.com/:path*",
        "destination": "https://koonmow.com/:path*",
        "permanent": true
      }
    ]
  }